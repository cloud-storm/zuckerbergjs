{
  "manifest_version": 2,
  "name": "Color Blind Utility by CloudStorm",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon_off.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "matches": [
        "<all_urls>"
      ],
      "js": ["tinycolor_1_3_0_min.js", "colorblind.js"]
    }
  ]
}